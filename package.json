{
  "name": "setup-custom-tool",
  "version": "1.0.5",
  "private": true,
  "description": "Download and unpack a tool of your choice",
  "main": "lib/main.js",
  "scripts": {
    "lint": "eslint --ext .ts --fix src",
    "clean": "rm -rf lib coverage",
    "build": "tsc",
    "dist": "ncc build",
    "test": "jest",
    "all": "npm run lint && npm run clean && npm run build && npm run dist && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex079/setup-custom-tool.git"
  },
  "keywords": [
    "actions",
    "tool",
    "setup"
  ],
  "author": "Alex079",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1",
    "@actions/glob": "^0",
    "@actions/tool-cache": "^1"
  },
  "devDependencies": {
    "@types/jest": "^26",
    "@types/node": "^13",
    "@typescript-eslint/parser": "^3",
    "@zeit/ncc": "^0",
    "eslint": "^7",
    "eslint-plugin-github": "^4",
    "eslint-plugin-jest": "^23",
    "jest": "^26",
    "jest-circus": "^26",
    "js-yaml": "^3",
    "ts-jest": "^26",
    "typescript": "^3"
  }
}
